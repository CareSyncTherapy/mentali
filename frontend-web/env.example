# CareSync Frontend Environment Variables
# Copy this file to .env and update with your actual values

# API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_APP_VERSION=1.0.0
VITE_APP_NAME=CareSync

# Analytics
VITE_GOOGLE_ANALYTICS_ID=G-[YOUR-ACTUAL-MEASUREMENT-ID]

# Error Tracking
VITE_SENTRY_DSN=https://[YOUR-SENTRY-DSN]@[YOUR-ORG].ingest.sentry.io/[YOUR-PROJECT]

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_DEBUG_MODE=false

# App Configuration
VITE_DEFAULT_LANGUAGE=he
VITE_SUPPORTED_LANGUAGES=he,ar,en
VITE_APP_ENVIRONMENT=development 